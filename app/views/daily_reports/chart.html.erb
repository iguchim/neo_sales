<div class="container">
<div class="row">

<div class="col-sm-6">
  <div class="col-sm-6">
    <h4 class="text-primary text-center">行動区分</h4>
  </div>

  <div class="col-sm-12">
    <% @action_data.each do |key, value| %>
      <div class="col-sm-3 text-right">
        <%= key %>
      </div>
      <div class="col-sm-12">
        <%= pie_chart @action_data[key] %>
      </div>
    <% end %>
  </div>
</div>

<div class="col-sm-6">
  <div class="col-sm-6">
    <h4 class="text-primary text-center">内容区分</h4>
  </div>

  <div class="col-sm-12">
    <% @contents_data.each do |key, value| %>
      <div class="col-sm-3 text-right">
        <%= key %>
      </div>
      <div class="col-sm-12">
        <%= pie_chart @contents_data[key] %>
      </div>
    <% end %>
  </div>
</div>

</div><!--row-->
</div><!--container-->
